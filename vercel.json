{
  "git": {
    "deploymentEnabled": {
      "main": true,
      "dev": false,
      "staging": false,
      "feature/*": false,
      "hotfix/*": false
    }
  },
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "framework": "nextjs",
  "functions": {
    "app/api/agents/messages/route.ts": {
      "maxDuration": 300
    },
    "app/api/leads/get-email/route.ts": {
      "maxDuration": 300
    },
    "app/api/leads/route.ts": {
      "maxDuration": 300
    },
    "app/api/leads/save/route.ts": {
      "maxDuration": 300
    },
    "app/api/leads/[id]/route.ts": {
      "maxDuration": 300
    }
  }
} 